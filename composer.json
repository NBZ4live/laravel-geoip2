{
    "name": "nbz4live/laravel-geoip2",
    "description": "Laravel wrapper for MaxMind GeoIP2",
    "keywords": [
        "laravel",
        "geoip",
        "geoip2",
        "maxmind"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Fayngold",
            "email": "sergey@faynhost.com"
        },
        {
            "name": "Jakub Srsen",
            "email": "jsrsen@talkative.uk"
        },
        {
            "name": "Dave Hennigar",
            "email": "dave.hennigar@gmail.com"
        },
        {
            "name": "Csanad Novak",
            "email": "csanad.novak@trendideas.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "geoip2/geoip2": "~2.0",
        "guzzlehttp/guzzle": "~6.1",
        "illuminate/config": "^5.0||^6.0",
        "illuminate/http": "^5.0||^6.0",
        "illuminate/support": "^5.0||^6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^4.0||^6.0",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Nbz4live\\LaravelGeoIP2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nbz4live\\LaravelGeoIP2\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Nbz4live\\LaravelGeoIP2\\ServiceProvider"
            ],
            "aliases": {
                "GeoIP2": "Nbz4live\\LaravelGeoIP2\\Facades\\GeoIP2"
            }
        }
    },
    "minimum-stability": "stable"
}
